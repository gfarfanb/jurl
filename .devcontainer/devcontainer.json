{
    "name": "Jurl",
    "build": {
        "context": "..",
        "dockerfile": "../Dockerfile"
    },
    "features": {
        "ghcr.io/devcontainers/features/git:1.2.0": {}
    },
    "customizations": {
        "vscode": {
